# SMTP Email Configuration
# Mail server host
VITE_SMTP_HOST=mail.grupogirassol.co.ao

# SMTP port (465 for SSL, 587 for TLS)
VITE_SMTP_PORT=465

# Email account for sending (no-reply address)
VITE_SMTP_USER=no-reply@grupogirassol.co.ao

# Password for the email account
VITE_SMTP_PASS=your_mailbox_password_here

# Admin email to receive contact form submissions
VITE_ADMIN_EMAIL=suporte@grupogirassol.co.ao

# Allowed origin for CORS (your website URL)
VITE_ALLOWED_ORIGIN=https://www.grupogirassol.co.ao
